#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Smallestlibrary/Desktop/Task_manager/node_modules/.pnpm/pnpm@10.12.1/node_modules/pnpm/bin/node_modules:/mnt/c/Users/Smallestlibrary/Desktop/Task_manager/node_modules/.pnpm/pnpm@10.12.1/node_modules/pnpm/node_modules:/mnt/c/Users/Smallestlibrary/Desktop/Task_manager/node_modules/.pnpm/pnpm@10.12.1/node_modules:/mnt/c/Users/Smallestlibrary/Desktop/Task_manager/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Smallestlibrary/Desktop/Task_manager/node_modules/.pnpm/pnpm@10.12.1/node_modules/pnpm/bin/node_modules:/mnt/c/Users/Smallestlibrary/Desktop/Task_manager/node_modules/.pnpm/pnpm@10.12.1/node_modules/pnpm/node_modules:/mnt/c/Users/Smallestlibrary/Desktop/Task_manager/node_modules/.pnpm/pnpm@10.12.1/node_modules:/mnt/c/Users/Smallestlibrary/Desktop/Task_manager/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/pnpx.cjs" "$@"
else
  exec node  "$basedir/../../bin/pnpx.cjs" "$@"
fi
